{
  "version": 2,
  "rewrites": [
    {
      "source": "/(admin/.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(raw/.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(blob/.*)",
      "destination": "/api/index.js"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
        }
      ]
    }
  ]
}

