{
  "version": 2,
  "rewrites": [
    { 
      "source": "/api/admin/(.*)", 
      "destination": "/api/index.js" 
    },
    { 
      "source": "/raw/(.*)", 
      "destination": "/api/index.js/raw/$1" 
    },
    { 
      "source": "/blob/(.*)", 
      "destination": "/api/index.js/blob/$1" 
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
        }
      ]
    }
  ]
}

